{
  "name": "@difuks/auth-backend",
  "version": "0.2.1",
  "description": "My blog admin",
  "repository": "https://github.com/DiFuks/fuks-blog",
  "author": "Dmitry Fuks",
  "license": "ISC",
  "main": "./dist/lib/index.js",
  "module": "./dist/lib/index.js",
  "types": "./dist/lib/index.d.ts",
  "scripts": {
    "build:lib": "cross-env NODE_ENV=development ts-node -r tsconfig-paths/register ./src/schemaBuilder.ts",
    "build": "ttsc --project tsconfig.build.json",
    "dev": "cross-env NODE_ENV=development ts-node-dev -r tsconfig-paths/register --transpile-only --inspect=0.0.0.0:5003 ./src/main.ts",
    "prod": "cross-env NODE_ENV=production node ./dist/build/main.js",
    "e2e": "cross-env NODE_ENV=development jest",
    "dev:migration:generate": "cross-env NODE_ENV=development bash ./.bin/generate-migration.sh",
    "prod:migration:run": "cross-env NODE_ENV=production yarn prod:typeorm migration:run",
    "dev:typeorm": "cross-env NODE_ENV=development node -r ts-node/register/transpile-only $(yarn bin typeorm) -d ./src/Config/utils/ormconfig.ts",
    "prod:typeorm": "cross-env NODE_ENV=production node $(yarn bin typeorm) -d ./dist/build/Config/utils/ormconfig.js"
  },
  "files": [
    "dist/lib",
    "README.md"
  ],
  "publishConfig": {
    "registry": "https://npm.pkg.github.com"
  },
  "devDependencies": {
    "@difuks/common": "workspace:0.1.1",
    "@nestjs-modules/mailer": "^1.6.1",
    "@nestjs/common": "^8.4.4",
    "@nestjs/core": "^8.4.4",
    "@nestjs/jwt": "^8.0.0",
    "@nestjs/passport": "^8.2.1",
    "@nestjs/platform-express": "^8.4.4",
    "@nestjs/swagger": "^5.2.1",
    "@nestjs/testing": "^8.4.4",
    "@nestjs/typeorm": "^8.0.3",
    "@nestlab/google-recaptcha": "^2.1.2",
    "@types/cookie-parser": "^1.4.2",
    "@types/express": "^4.17.13",
    "@types/jest": "^27.4.1",
    "@types/nodemailer": "^6.4.4",
    "@types/passport-jwt": "^3.0.6",
    "@types/passport-local": "^1.0.34",
    "@types/qs": "^6.9.7",
    "@types/supertest": "^2.0.12",
    "@types/uuid": "^8.3.4",
    "argon2": "^0.28.5",
    "axios": "^0.25.0",
    "class-transformer": "^0.5.1",
    "class-validator": "^0.13.2",
    "cookie-parser": "^1.4.6",
    "cross-env": "^7.0.3",
    "date-fns": "^2.28.0",
    "express": "^4.17.3",
    "google-auth-library": "^7.14.1",
    "jest": "^27.5.1",
    "nestjs-i18n": "^9.1.3",
    "nodemailer": "^6.7.5",
    "openapi-client-axios": "^5.1.2",
    "openapi-client-axios-typegen": "^5.0.2",
    "passport": "^0.5.2",
    "passport-custom": "^1.1.1",
    "pg": "^8.7.3",
    "qs": "^6.10.5",
    "reflect-metadata": "^0.1.13",
    "rxjs": "^7.5.5",
    "sqlite3": "^5.0.5",
    "supertest": "^6.2.2",
    "swagger-ui-express": "^4.3.0",
    "ts-jest": "^27.1.4",
    "ts-node": "^10.7.0",
    "ts-node-dev": "^1.1.8",
    "tsconfig-paths": "^3.14.1",
    "ttypescript": "^1.5.13",
    "typeorm": "^0.3.6",
    "typescript": "^4.6.3",
    "typescript-transform-paths": "^3.3.1",
    "uuid": "^8.3.2"
  },
  "peerDependencies": {
    "axios": "^0.27.2",
    "openapi-client-axios": "^5.1.2"
  }
}
